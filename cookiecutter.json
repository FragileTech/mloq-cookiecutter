    "command_line_interface": ["click", "plain", "argparse",  "no"],
    "command_line_interface_bin_name": "{{ cookiecutter.distribution_name }}",
    "codecov": ["yes", "no"],
    "sphinx_docs": ["yes", "no"],
    "sphinx_docs_hosting": "https://{{ cookiecutter.repo_name|replace('.', '') }}.readthedocs.io/",
